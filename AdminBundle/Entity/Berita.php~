<?php

namespace SMA\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Berita
 */
class Berita
{
    /**
     * @var \DateTime
     */
    private $tanggal;

    /**
     * @var string
     */
    private $judul;

    /**
     * @var string
     */
    private $konten;

    /**
     * @var string
     */
    private $penulis;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set tanggal
     *
     * @param \DateTime $tanggal
     * @return Berita
     */
    public function setTanggal($tanggal)
    {
        $this->tanggal = $tanggal;

        return $this;
    }

    /**
     * Get tanggal
     *
     * @return \DateTime 
     */
    public function getTanggal()
    {
        return $this->tanggal;
    }

    /**
     * Set judul
     *
     * @param string $judul
     * @return Berita
     */
    public function setJudul($judul)
    {
        $this->judul = $judul;

        return $this;
    }

    /**
     * Get judul
     *
     * @return string 
     */
    public function getJudul()
    {
        return $this->judul;
    }

    /**
     * Set konten
     *
     * @param string $konten
     * @return Berita
     */
    public function setKonten($konten)
    {
        $this->konten = $konten;

        return $this;
    }

    /**
     * Get konten
     *
     * @return string 
     */
    public function getKonten()
    {
        return $this->konten;
    }

    /**
     * Set penulis
     *
     * @param string $penulis
     * @return Berita
     */
    public function setPenulis($penulis)
    {
        $this->penulis = $penulis;

        return $this;
    }

    /**
     * Get penulis
     *
     * @return string 
     */
    public function getPenulis()
    {
        return $this->penulis;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
