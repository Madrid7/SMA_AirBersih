<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SMA\AdminBundle\Entity\Tugas" table="tugas">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tanggal" type="date" column="tanggal" nullable="false"/>
    <field name="userGuru" type="string" column="user_guru" length="20" nullable="false"/>
    <field name="namaGuru" type="string" column="nama_guru" length="30" nullable="false"/>
    <field name="kelas" type="string" column="kelas" length="10" nullable="false"/>
    <field name="jurusan" type="string" column="jurusan" length="20" nullable="false"/>
    <field name="judul" type="string" column="judul" length="100" nullable="false"/>
    <field name="isi" type="text" column="isi" nullable="false"/>
    <field name="mataPel" type="string" column="mata_pel" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
