<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SMA\AdminBundle\Entity\Berita" table="berita">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tanggal" type="date" column="tanggal" nullable="false"/>
    <field name="judul" type="string" column="judul" length="100" nullable="false"/>
    <field name="konten" type="text" column="konten" nullable="false"/>
    <field name="penulis" type="string" column="penulis" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
