{% extends "SMAUserBundle:StudentPage:StudentTemplate.html.twig" %}

{% block content %}
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>No</th>
            <th>Mata Pelajaran</th>
            <th>Judul Tugas</th>
            <th>Guru Penguji</th>
            <th>Nilai</th>                                         
        </tr>
    </thead>   
    <tbody>
        {% for i in nilai %}
        <tr>
            <td>{{loop.index}}</td>
            <td>{{i.matapelajaran}}</td>
            <td>{{i.judultugas}}</td>
            <td>{{i.namaGuru}}</td>
            <td>{{i.nilai}}</td>
        </tr>
       {% endfor %} 
    </tbody>
</table>
{% endblock %}