{% extends "SMAUserBundle:TeacherPage:TeacherTemplate.html.twig" %}

{% block content %}
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th>No</th>
            <th>Mata Pelajaran</th>
            <th>Judul</th>
            <th>NIS</th>
            <th>Nama Siswa</th>
            <th>Nilai</th>                                         
        </tr>
    </thead>   
    <tbody>
        {% for i in nilai %}
        <tr>
            <td>{{loop.index}}</td>
            <td>{{i.matapelajaran}}</td>
            <td>{{i.judultugas}}</td>
            <td>{{i.nis}}</td>
            <td>{{i.namaSiswa}}</td>
            <td>{{i.nilai}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}